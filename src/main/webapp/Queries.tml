<html t:type="layout" title="Atributes"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd" xmlns:p="tapestry:parameter">
	<div t:type="Grid" source="queries" t:exclude="queryId" t:row="query">
		<t:parameter name="nameCell">
			<a href="#" t:type="PageLink" t:page="Query" context="query.queryId">${query.name}</a>
		</t:parameter>
		<t:parameter name="textCell">
			<t:outputRaw value="querytext"/>
		</t:parameter>
	</div>
</html>